{
  "scripts": {
    "shadow:watch": "shadow-cljs server",
    "css:watch": "onchange 'public/js/*' -- npm run tw",
    "shadow:release": "shadow-cljs release app",
    "tw": "npx tailwindcss -i global.css -o public/css/main.css",
    "dev": "run-p -l *:watch",
    "release": "run-s *:release"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "shadow-cljs": "^2.23.3",
    "tailwindcss": "^3.3.2"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
